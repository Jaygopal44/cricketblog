<%-include('../../header.ejs')%>
                    <%-include('../sidebar.ejs')%>
                    <div class="col-md-9">
                        <div class="dash-right">
                            <div class="dash-header">
                                <div class="dash-title">
                                    <h1>Create Blog</h1>
                                </div>
                            </div>
                            <div class="blog-form">
                                <form action="/category/update/<%=data._id %>" method="post">
                                    <div class="form-group form-row">
                                        <label class="col-md-3">Title <span
                                                class="required">*</span></label>
                                        <div class="col-md-9">
                                            <input type="text"
                                                class="form-control"
                                                placeholder="Title" name="title" value="<%=data.title %>" required>
                                        </div>
                                    </div>
                                 
                                    <div class="form-group form-row">
                                        <label class="col-md-3"></label>
                                        <div class="col-md-9">
                                            <button type="submit" class="btn
                                                btn-primary">Update</button>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <%-include('../../footer.ejs')%>
